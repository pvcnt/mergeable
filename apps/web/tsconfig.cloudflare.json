{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.vite.json",
  "include": [
    "src/**/*",
    "**/.server/**/*",
    "**/.client/**/*",
    ".react-router/types/**/*",
    "cloudflare/**/*",
    "worker-configuration.d.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "types": ["node", "@react-router/cloudflare"],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "noEmit": true,
  }
}
{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "check-types": {
      "dependsOn": ["^transit"]
    },
    "lint": {
      "dependsOn": ["check-types"]
    },
    "test": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}